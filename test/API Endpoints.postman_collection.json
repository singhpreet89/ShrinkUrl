							"name": "1. Post (JSON) - http://iitd.ac.in",
							"event": [
								{
									"listen": "prerequest",
									"script": {
										"id": "81d7095b-f96e-4158-b757-659eea980e81",
										"exec": [
											"pm.variables.set(\"iitDelhi\", \"http://www.iitd.ac.in\");"
										],
										"type": "text/javascript"
									}
								},
								{
									"listen": "test",
									"script": {
										"id": "a3ed222d-2cc7-4a69-8496-aea59f7d5776",
										"exec": [
											"const response = pm.response.json();",
											"",
											"pm.test(\"Verify status code\", function() {",
											"    pm.response.to.have.status(201);",
											"});",
											"",
											"pm.test(\"Check data\", function() {",
											"    pm.expect(response.url.url).to.equal(pm.variables.get('iitDelhi'));",
											"    ",
											"    // pm.environment.set('iitDelhiId', response.url._id);              // To be used in the PATCH & DELETE Request for iitDelhi",
											"    pm.environment.set(`ID--${response.url.url}`, response.url._id);    // To be used in the PATCH & DELETE Request for iitDelhi",
											"});",
											"",
											"pm.variables.unset('iitDelhi'); // Unset because no longer required",
											""
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [],
								"body": {
									"mode": "raw",
									"raw": "{\r\n\t\"fullUrl\" : \"{{iitDelhi}}\"\r\n}",
