						{
							"name": "3. Post with Pre-request script",
							"event": [
								{
									"listen": "prerequest",
									"script": {
										"id": "afb40797-45af-4ae7-a76b-043f84c01cfd",
										"exec": [
											"let urls = pm.environment.get('urls');",
											"",
											"if(!urls) {",
											"    urls = [",
											"        \"https://www.google.com\",",
											"        \"http://www.gmail.com\",",
											"        \"https://www.yahoo.com\",",
											"        \"https://www.yahoo.in\",",
											"        \"http://www.samsung.com\",",
											"        \"http://www.digit.in\",",
											"        \"http://www.nitj.ac.in\",",
											"    ];",
											"}",
											"",
											"let fullUrl = urls.shift();",
											"pm.environment.set(\"urlToPost\", fullUrl);",
											"",
											"pm.environment.set(\"urls\", urls);"
										],
										"type": "text/javascript"
									}
								},
								{
									"listen": "test",
									"script": {
										"id": "e7d7f487-d87a-4e19-beba-c8b9cd356111",
										"exec": [
											"let urls = pm.environment.get('urls');",
											"",
											"if(urls && urls.length > 0) {",
											"    postman.setNextRequest(\"3. Post with Pre-request script\");",
											"",
											"    // Run test cases for n - 1 Url's defined in the Pre-request Script",
											"    runTestCases();",
											"} ",
											"",
											"else {",
											"    // Run test cases for the last Url defined in the Pre-request Script",
											"    runTestCases();",
											"    ",
											"    pm.environment.unset('urlToPost');",
											"    pm.environment.unset('urls');",
											"    // postman.setNextRequest('2. Post (JSON) - http://iitd.ac.in');",
											"    postman.setNextRequest(null);",
											"}",
											"",
											"",
											"function runTestCases() {",
											"    /* Test cases for each URL created */",
											"    const response = pm.response.json();",
											"",
											"    pm.test(\"Verify status code\", function() {",
											"        pm.response.to.have.status(201);",
											"    });",
											"    ",
											"    pm.test(\"Check data\", function() {",
											"        pm.expect(response.url.url).to.equal(pm.environment.get(\"urlToPost\"));",
											"        ",
											"        pm.environment.set(`ID--${response.url.url}`, response.url._id);    // To be used in the PATCH & DELETE Request for the corresponding URL's",
											"    });",
											"    /* */",
											"}"
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [],
								"body": {
									"mode": "raw",
